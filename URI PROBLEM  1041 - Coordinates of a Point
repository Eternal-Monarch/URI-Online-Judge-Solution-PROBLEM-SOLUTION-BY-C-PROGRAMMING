#include<stdio.h>
float main()
{
    float a,b,c,d,e;
    scanf("%f %f",&a,&b);
    if(a==0 && b==0)
    {
        printf("Origem\n");
    }
    else if(a==0)
    {
        printf("Eixo Y\n");
    }
    else if(b==0)
    {
        printf("Eixo X\n");
    }
    else if(a>0 && b>0)
    {
        printf("Q1");
    }
    else if(a< 0&& b>0)
    {
        printf("Q2");
    }
    else if(a<0 && b<0)
    {
        printf("Q3");
    }
    else if(a>0 && b<0)
    {
        printf("Q4");
    }
    return 0;
}
